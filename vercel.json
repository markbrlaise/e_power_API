{
    "version": 2,
    "installCommand": "npm install",
    "buildCommand": "npm start",
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node-server"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.js"
        }
    ]
}
