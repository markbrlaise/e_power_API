    "name": "e_power_api",
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node",
            "dependencies": {
                "bcryptjs": "^2.4.3",
                "body-parser": "^1.19.0",
                "cookie-parser": "^1.4.6",
                "cors": "^2.8.5",
                "dotenv": "^8.2.0",
                "express": "^4.17.1",
                "joi": "^17.2.1",
                "jsonwebtoken": "^8.5.1",
                "mongoose": "^5.10.3",
                "mongoose-paginate": "^5.0.3",
                "multer": "^1.4.2",
